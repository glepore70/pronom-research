#, c-format
msgid "Raw PNM image type is invalid"
msgstr "PNM irudi-mota gordina baliogabea da"

#: gdk-pixbuf/io-pnm.c:562 gdk-pixbuf/io-pnm.c:604
#, c-format
msgid "PNM image format is invalid"
msgstr "PNM irudi-formatua baliogabea da"

