<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="simpleNoMFC"
	ProjectGUID="{01010B44-658B-4502-8DDB-70E15BBD649C}">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="i386 Release|Win32"
			OutputDirectory="$(HEIDIBIN)"
			IntermediateDirectory="$(HEIDIOBJ)\simpleNoMFC"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="$(USRCFLAGS)"
				InlineFunctionExpansion="1"
				AdditionalIncludeDirectories="..\..\source"
				PreprocessorDefinitions="WIN32,NDEBUG,_WINDOWS"
				StringPooling="TRUE"
				RuntimeLibrary="2"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="main.h"
				PrecompiledHeaderFile="$(HEIDIOBJ)\simpleNoMFC\simpleNoMFC.pch"
				AssemblerListingLocation="$(HEIDIOBJ)\simpleNoMFC/"
				ObjectFile="$(HEIDIOBJ)\simpleNoMFC/"
				ProgramDataBaseFileName="$(HEIDIOBJ)\simpleNoMFC/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 $(USRLFLAGS)"
				AdditionalDependencies="$(HEIDILIB)\heidi$(HEIDIVER).lib $(HEIDILIB)\mtl$(HEIDIVER).lib $(HEIDILIB)\texture$(HEIDIVER).lib odbc32.lib odbccp32.lib"
				OutputFile="$(HEIDIBIN)\simpleNoMFC.exe"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				ProgramDatabaseFile="$(HEIDIBIN)\simpleNoMFC.pdb"
				SubSystem="2"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName="$(HEIDILIB)\simpleNoMFC.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="i386 Debug|Win32"
			OutputDirectory="$(HEIDIBIN)"
			IntermediateDirectory="$(HEIDIOBJ)\simpleNoMFC"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="$(USRCFLAGS)"
				Optimization="0"
				AdditionalIncludeDirectories="..\..\source"
				PreprocessorDefinitions="WIN32,_DEBUG,_WINDOWS"
				RuntimeLibrary="3"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="main.h"
				PrecompiledHeaderFile="$(HEIDIOBJ)\simpleNoMFC\simpleNoMFC.pch"
				AssemblerListingLocation="$(HEIDIOBJ)\simpleNoMFC/"
				ObjectFile="$(HEIDIOBJ)\simpleNoMFC/"
				ProgramDataBaseFileName="$(HEIDIOBJ)\simpleNoMFC/"
				BrowseInformation="1"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386 $(USRLFLAGS)"
				AdditionalDependencies="$(HEIDILIB)\heidi$(HEIDIVER).lib $(HEIDILIB)\mtl$(HEIDIVER).lib $(HEIDILIB)\texture$(HEIDIVER).lib odbc32.lib odbccp32.lib"
				OutputFile="$(HEIDIBIN)\simpleNoMFC.exe"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="$(HEIDIBIN)\simpleNoMFC.pdb"
				SubSystem="2"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName="$(HEIDILIB)\simpleNoMFC.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;r;odl;hpj;bat;for;f90">
			<File
				RelativePath=".\heidi_glue.cpp">
			</File>
			<File
				RelativePath=".\main.cpp">
			</File>
			<File
				RelativePath=".\simpleNoMFC.rc">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl;fi;fd">
			<File
				RelativePath=".\heidi_glue.h">
			</File>
			<File
				RelativePath=".\main.h">
			</File>
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;cnt;rtf;gif;jpg;jpeg;jpe">
			<File
				RelativePath=".\SimpleNoMFC.ICO">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
