    <math xmlns="http://www.w3.org/1998/Math/MathML" id="10" cmeta:id="10">
      <apply id="V1_calculation">
        <eq/> 
        <ci> V1 </ci>
        <apply>
          <times/>
          <ci> a4 </ci>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> Ca_i </ci>
              <ci> x_000 </ci>
            </apply>
            <apply>
              <times/>
              <ci> d4 </ci>
              <ci> x_001 </ci>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="V2_calculation">
        <eq/> 
        <ci> V2 </ci>
        <apply>
          <times/>
          <ci> a4 </ci>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> Ca_i </ci>
              <ci> x_010 </ci>
            </apply>
            <apply>
              <times/>
              <ci> d4 </ci>
              <ci> x_011 </ci>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="V3_calculation">
        <eq/> 
        <ci> V3 </ci>
        <apply>
          <times/>
          <ci> a5 </ci>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> Ca_i </ci>
              <ci> x_000 </ci>
            </apply>
            <apply>
              <times/>
              <ci> d5 </ci>
              <ci> x_010 </ci>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="V4_calculation">
        <eq/> 
        <ci> V4 </ci>
        <apply>
          <times/>
          <ci> a5 </ci>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> Ca_i </ci>
              <ci> x_001 </ci>
            </apply>
            <apply>
              <times/>
              <ci> d5 </ci>
              <ci> x_011 </ci>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  
